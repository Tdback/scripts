#!/bin/sed -f

################################################################################
# @author      : Tyler Dunneback (tylerdback@pm.me)
# @file        : state_to_abbr
# @created     : Tue Aug 15 09:09:02 PM EDT 2023
#
# @description : Sed script that renames state names to their abbreviations.
################################################################################

s/Alabama/AL/g
s/Alaska/AK/g
s/Arizona/AZ/g
s/Arkansas/AR/g
s/California/CA/g
s/Colorado/CO/g
s/Connecticut/CT/g
s/Delaware/DE/g
s/Florida/FL/g
s/Georgia/GA/g
s/Hawaii/HI/g
s/Idaho/ID/g
s/Illinois/IL/g
s/Indiana/IN/g
s/Iowa/IA/g
s/Kansas/KS/g
s/Kentucky/KY/g
s/Louisiana/LA/g
s/Maine/ME/g
s/Maryland/MD/g
s/Massachusetts/MA/g
s/Michigan/MI/g
s/Mississippi/MS/g
s/Missouri/MO/g
s/Montana/MT/g
s/Nebraska/NE/g
s/Nevada/NV/g
s/New Hampshire/NH/g
s/New Jersey/NJ/g
s/New Mexico/NM/g
s/New York/NY/g
s/North Carolina/NC/g
s/North Dakota/ND/g
s/Ohio/OH/g
s/Oklahoma/OK/g
s/Oregon/OR/g
s/Pennsylvania/PA/g
s/Rhode Island/RI/g
s/South Carolina/SC/g
s/South Dakota/SD/g
s/Tennessee/TN/g
s/Texas/TX/g
s/Utah/UT/g
s/Vermont/VT/g
s/Virginia/VA/g
s/Washington/WA/g
s/West Virginia/WV/g
s/Wisconsin/WI/g
s/Wyoming/WY/g
